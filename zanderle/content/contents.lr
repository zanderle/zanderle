print("Hi!")

# Create zanderle object

from .models import FullStackWebDeveloper
from .techs import django, python, d3js

zanderle = FullStackWebDeveloper.objects.create(
    name = "Žan Anderle",
    ux_developer = True,
    background = "MechE",
    friendly = True,
    community = True,
    github = "https://github.com/zanderle",
    twitter = "https://twitter.com/z_anderle"
)
zanderle.save()
zanderle.techs.add(django, python, d3js)
music = Interest.objects.create(
    name="Music",
    resources={'blog': 'http://musingsonmusic.com', 'band': 'http://salonski.tumblr.com/'}
)
dataviz = Interest.objects.create(
    name="Data visualization",
    resources={'Louis Armstrong Discography': 'http://musingsonmusic.com/2015/08/29/Louis-Armstrong-Discography/'}
)
zanderle.interests.add(music, dataviz)

